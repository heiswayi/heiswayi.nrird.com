{
  "name": "heiswayi.nrird.com",
  "version": "1.0.1",
  "description": "Heiswayi Nrird's Personal Blog. It is a static site, built with Jekyll and deployed by [Netlify](https://www.netlify.com/).",
  "main": "gulpfile.js",
  "dependencies": {
    "gulp-clean-css": "^3.9.4",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-plumber": "^1.2.0",
    "gulp-connect": "^5.5.0",
    "gulp-rename": "^1.3.0",
    "gulp-sass": "^4.0.1",
    "gulp-util": "^3.0.8"
  },
  "devDependencies": {
    "purgecss": "^3.1.3"
  },
  "scripts": {
    "build": "npm run jekyll-build",
    "start": "npm run jekyll-serve",
    "prod": "npm run jekyll-prod",
    "preinstall": "npx npm-force-resolutions",
    "jekyll-prod": "JEKYLL_ENV=production bundle exec jekyll build",
    "jekyll-build": "bundle exec jekyll build --config=_config.yml,_config.dev.yml",
    "jekyll-serve": "bundle exec jekyll serve --config=_config.yml,_config.dev.yml",
    "bundle-install": "bundle install"
  },
  "resolutions": {
    "graceful-fs": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://heiswayi@bitbucket.org/heiswayi/heiswayi.nrird.com.git"
  },
  "author": "Heiswayi Nrird",
  "license": "MIT",
  "homepage": "https://bitbucket.org/heiswayi/heiswayi.nrird.com#readme",
  "keywords": [
    "heiswayi-nrird",
    "personal-blog",
    "jekyll",
    "static-site"
  ]
}
